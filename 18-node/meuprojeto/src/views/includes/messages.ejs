
<% if(errors.length > 0) { %>
    <div class="alert alert-danger">
    <% errors.forEach(erro => { %>
    <%= erro %><br>
    <% }); %>
    </div>
<% } %>

<% if(success.length > 0) { %>
    <div class="alert alert-success">
    <% success.forEach(msg => { %>
    <%= msg %><br>
    <% }); %>
    </div>
<% } %>